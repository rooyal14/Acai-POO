
public class Acai{
	
	String nome;
	double preco;
	int tamanho;
	String[] acompanhamentos;
	
	public Acai(String nome, double preco,int tamanho, String[] acompanhamentos)
	{
		this.nome = nome;
		this.preco = preco;
		this.tamanho = tamanho;
		this.acompanhamentos = acompanhamentos;
	}
	
	
	
	
	
	
}
